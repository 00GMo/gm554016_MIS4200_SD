@model gm554016.Models.Consoles

@{
    ViewBag.Title = "Game Console Details";
}

<h2>Game Console Details</h2>

<div>
    
    <dl class="dl-horizontal">
        <dt>
            Developer Name
        </dt>

        <dd>
            @Html.DisplayFor(model => model.developer.developerName)
        </dd>

        <dt>
            Console Name
        </dt>

        <dd>
            @Html.DisplayFor(model => model.consoleName)
        </dd>

        <dt>
            Release Date
        </dt>

        <dd>
            @Html.DisplayFor(model => model.releaseDate)
        </dd>

        <dt>
            Description
        </dt>

        <dd>
            @Html.DisplayFor(model => model.description)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Edit", "Edit", new { id = Model.consolesID }) |
    @Html.ActionLink("Back to Game Console List", "Index")
</p>
